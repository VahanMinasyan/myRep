@using MyStoryWeb.Models
@model  MyStoriesViewModel

@{
    ViewBag.Title = "Index";
}

<a href="/MyStories/Index">Create New Story</a>
@for (int i = 0; i < @Model.Stories.Count(); i++)
{
    <ul>
        <li>ID: @Model.Stories[i].ID   Title: @Model.Stories[i].Title</li>
        <li storyId=@Model.Stories[i].Title><a href="/MyStories/Index">Details</a></li>
        <li storyid=@Model.Stories[i].Title><a href="/MyStories/Index">Edit</a></li>
    </ul>
}
